  "homepage": "https://my-materials-manager.netlify.app",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "build": "babel src/server -d dist && webpack --config webpack.config.babel.js",
    "client": "cross-env  WEBPACK_ENV=development webpack serve --config webpack.config.babel.js --devtool eval-cheap-source-map --hot",
    "server": "nodemon --exec babel-node src/server/main.js --watch",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
